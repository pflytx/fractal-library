{{#>@full-width-wrapper
  color="text"
  className="cs--filterable js-cs--filterable"
  rhythm="large"
  id="cs--filterable"
}}

  <div class="cs--filterable__head">
    <div class="primary rhythm">
      {{#>@heading variant="h3" className="color--white"}}
        Weâ€™ve built successful partnerships across multiple industries, with fleets from 10 to 500 + vehicles.
      {{/@heading}}

      <p class="color--white">Find out how we've helped companies just like yours.</p>
    </div>

    <div class="secondary">
      <div class="form-wrapper form--inline js-form--filters">
         <fieldset class="rhythm--small">
          <label for="filter--industry" class="color--white">Industry</label>
          <div class="input--select transparent">
            <select class="transparent bg--text" id="filter--industry" name="industry">
              <option value="All">All</option>
              <option value="Government">Government</option>
              <option value="Construction">Construction</option>
              <option value="Trucking">Trucking</option>
              <option value="Waste">Waste</option>
            </select>
          </div>
        </fieldset>
         <fieldset class="rhythm--small">
          <label for="filter--fleetsize" class="color--white">Fleet Size</label>
          <div class="input--select transparent">
            <select class="transparent bg--text" id="filter--fleetsize" name="fleetsize">
              <option value="All">All</option>
              <option value="10-49">10-49</option>
              <option value="50-499">50-499</option>
              <option value="500+">500+</option>
            </select>
          </div>
        </fieldset>
         <fieldset class="rhythm--small">
          <label for="filter--result" class="color--white">Result</label>
          <div class="input--select transparent">
            <select class="transparent bg--text" id="filter--result" name="result">
              <option value="All">All</option>
              <option value="Reduce Costs">Reduce Costs</option>
              <option value="Manage Fleet Risk">Manage Fleet Risk</option>
              <option value="Improved Driver Performance">Improved Driver Performance</option>
            </select>
          </div>
        </fieldset>
      </div>
    </div>
  </div>

  <div class="layout--4up layout--cs-list" data-initial-limit="8">
    {{#>@card--case-study
